<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sqlitecrud">

    <application
        android:allowBackup="true"
        android:fullBackupContent="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SQLiteCRUD"
        android:usesCleartextTraffic="true">
        <activity android:name=".activity.UpdateMemoryActivity"></activity>
        <activity android:name=".activity.NewMemoryActivity" />
        <activity android:name=".activity.MemoryActivity" />
        <activity android:name=".food.FoodListActivity" />
        <activity android:name=".food.FoodActivity" />
        <activity android:name=".activity.CameraActivity" />
        <activity android:name=".activity.LatlngActivity" />
        <activity android:name=".activity.PlaceMapActivity" />
        <activity android:name=".activity.PlaceActivity" />
        <activity android:name=".activity.RawMapActivity" />
        <activity android:name=".activity.CurrentPlaceActivity" />
        <activity android:name=".activity.TesMapActivity" />
        <activity android:name=".activity.OjekActivity" />
        <activity android:name=".activity.PlaceAutoCompleteActivity" />
        <activity android:name=".activity.DirectionActivity" />
        <activity android:name=".activity.MapActivity" />
        <activity android:name=".activity.HistoryActivity" />
        <activity android:name=".activity.ModifyTeacherActivity" />
        <activity android:name=".activity.ModTeacherActivity" />
        <activity android:name=".activity.AddCourseTeacherActivity" />
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <service android:name=".activity.FetchAddressIntentService" />
    </application>
    <uses-feature
        android:name="android.hardware.camera"
        android:required="false" />

    <uses-feature android:name="android.hardware.camera.autofocus" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>